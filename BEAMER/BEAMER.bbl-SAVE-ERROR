      \strng{namehash}{d724f751ca64398a247d27c42dfbf314}
      \strng{fullhash}{d724f751ca64398a247d27c42dfbf314}
      \strng{bibnamehash}{d724f751ca64398a247d27c42dfbf314}
      \strng{authorbibnamehash}{d724f751ca64398a247d27c42dfbf314}
